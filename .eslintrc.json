{
  "root": true,
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true,
    "es6": true,
    "es2017": true
  },
  "rules": {
    "no-undef-init": 1,
    "no-duplicate-case": 1,
    "no-empty": 1,
    "no-extra-semi": 1,
    "no-func-assign": 1,
    "no-irregular-whitespace": 1,
    "no-unreachable": 1,
    "curly": 1,
    "dot-notation": 1,
    "eqeqeq": 1,
    "no-empty-function": 1,
    "no-multi-spaces": 1,
    "no-mixed-spaces-and-tabs": 1,
    "no-trailing-spaces": 1,
    "default-case": 1,
    "no-fallthrough": 1,
    "no-unused-vars": 1,
    "no-use-before-define": 1,
    "no-redeclare": 1,
    "brace-style": 1,
    "indent": [1, 2],
    "quotes": [1, "single"],
    "semi": [1, "always"],
    "radix": "off"
  },
  "extends": ["prettier"]
}
